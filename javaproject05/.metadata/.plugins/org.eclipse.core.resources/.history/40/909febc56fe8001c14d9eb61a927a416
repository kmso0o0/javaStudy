package org.java.loop;

public class For2 {
	public static void main(String[] args) {
		System.out.println("for문");
		// 구구단
//		단 2 3456789
//		하 123456789
		for(int i=2; i<10; i++) {
			System.out.println(i+"단입니다");
			for(int j=1; j<10; j++) {
				System.out.println(i+"*"+j+"="+(i*j));
			}
		}
		
		
	}
}
