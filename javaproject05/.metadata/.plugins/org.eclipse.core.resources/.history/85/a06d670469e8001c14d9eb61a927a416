package org.java.loop;

public class For1 {
	public static void main(String[] args) {
		System.out.println("for문");
		
		for(int i=0;i<10;i++) {
//			System.out.println(i);
//			System.out.println(i+" ");
//			1~10까지 콘솔에 출력
			System.out.println((i+1)+",");
//			int i2=i+1;
//			System.out.println(i2);
		}
	}
}
